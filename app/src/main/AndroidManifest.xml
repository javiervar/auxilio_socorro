<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.kira.auxiliosocorro">


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!--agregando los pemisos necesarios -->
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" /><!--escribir almacenamiento interno-->
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" /><!--leer almacenamiento interno-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /><!--saber si esta conectado a internet o datos-->
    <uses-permission android:name="android.permission.INTERNET" /><!--conectarse a internet-->
    <uses-permission android:name="android.permission.VIBRATE" /><!--vibracion del dispositivo-->
    <uses-permission android:name="android.permission.SEND_SMS"/><!--enviar mensajes-->
    <uses-permission android:name="android.permission.WRITE_SMS"/><!--escribir mensajes-->
    <uses-permission android:name="android.permission.READ_SMS"/><!--leer mensajes-->
    <uses-permission android:name="android.permission.RECEIVE_SMS"/><!--recibir mensajes-->

    <uses-permission android:name="android.permission.READ_CONTACTS" /><!--acceder a los contactos-->
    <uses-permission android:name="com.tp.lib.tp.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <!--uses-permission android:name="android.permission." /-->


    <application
        android:allowBackup="true"
        android:icon="@drawable/auxilio"
        android:label="@string/app_name"
        android:roundIcon="@drawable/auxilio"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--
        android:name="com.google.android.geo.API_KEY"
        android:name="com.google.android.maps.V2.API_KEY"

        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>